plugins {
    id 'java'
    id 'eclipse' // use it for eclipse project
}

group 'com.javabydeveloper'
version '1.0-SNAPSHOT'

sourceCompatibility = 1.8

test {
    useJUnitPlatform()
}

repositories {
    mavenCentral()
}

dependencies {
    testImplementation group: 'org.junit.jupiter', 
    name: 'junit-jupiter', version: '5.6.0'
}